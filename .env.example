# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_VAPID_KEY=your_vapid_key

# Development Environment Settings
# Set to 'true' to disable Service Worker registration in unsupported environments
VITE_DISABLE_FCM_SW_REGISTRATION=false

# FCM Server Key Configuration
# This key should be set in Supabase Edge Functions Environment Variables
# Get it from Firebase Console > Project Settings > Cloud Messaging > Server Key
FCM_SERVER_KEY=your_fcm_server_key_here